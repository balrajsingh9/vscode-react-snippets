{
  "name": "react-snippets",
  "version": "1.0.1",
  "description": "Snippets for React.js--only for Javascript",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "vscode",
    "snippets",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dexter-69/vscode-react-snippets.git"
  },
  "author": "dexter-69",
  "license": "MIT",
  "contributes": {
    "snippets": [{
      "language": "javascript",
      "path": "./snippets.json"
    }, {
      "language": "javascriptreact",
      "path": "./snippets.json"
    }]
  },
  "activationEvents": [
    "onCommand:extension.snippetSearch"
  ],
  "extensionKind": "ui",
  "icon": "images/logo.png",
  "categories": [
    "Snippets"
  ],
  "publisher": "balraj-99",
  "engines": {
    "vscode": "^1.34.0"
  },
  "devDependencies": {
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.34.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1"
  }
}